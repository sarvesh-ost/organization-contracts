{
  "name": "@openstfoundation/organization-contracts",
  "version": "1.0.0-beta.1",
  "description": "Organization contracts manage application permissions",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile",
    "compile-all": "truffle compile --all",
    "test": "truffle test",
    "ganache": "sh tools/runGanacheCli.sh",
    "build-package": "node tools/build_package.js",
    "prepack": "npm run compile-all && npm run build-package",
    "lint": "eslint test tools -c .eslintrc.json --ext js",
    "generate": "abi-gen --abis './build/contracts/*.json' --out './interacts/' --template 'node_modules/@0x/abi-gen-templates/contract.handlebars'  --partials 'node_modules/@0x/abi-gen-templates/partials/*.handlebars'",
    "compile:artifacts": "sol-compiler",
    "contract-gen": "contracts-gen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenST/organization-contracts.git"
  },
  "keywords": [
    "OpenST",
    "OST",
    "web3",
    "Ethereum",
    "ACL",
    "permissions"
  ],
  "author": "OpenST Foundation Ltd.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpenST/organization-contracts/issues"
  },
  "homepage": "https://openst.org/",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@0x/abi-gen": "^2.0.9",
    "@0x/abi-gen-templates": "^2.0.2",
    "@0x/base-contract": "^5.0.5",
    "@0x/contracts-gen": "^1.0.8",
    "@0x/sol-compiler": "^3.1.6",
    "@0x/subproviders": "^4.0.5",
    "@0x/typescript-typings": "^4.2.2",
    "@0x/utils": "^4.3.1",
    "@0x/web3-wrapper": "^6.0.5",
    "@types/node": "^11.11.3",
    "abi-decoder": "1.2.0",
    "assert": "1.4.1",
    "bn.js": "4.11.8",
    "chai": "4.2.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "ethereum-types": "^2.1.2",
    "ethers": "^4.0.27",
    "ganache-cli": "6.1.8",
    "lodash": "^4.17.11",
    "mocha": "5.2.0",
    "truffle": "5.0.6",
    "typechain": "^0.3.14",
    "typescript": "^3.3.3333",
    "web3": "1.0.0-beta.36"
  },
  "config": {
    "abis:comment": "This list is auto-generated by contracts-gen. Don't edit manually.",
    "abis": "./generated-artifacts/@(Organization|OrganizationInterface|Organized).json"
  }
}
